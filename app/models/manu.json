    {
      "label": "联系人",
      "name": "contact_name",
      "type": "string",
      "length": 200,
      "comment": "联系人",
      "nullable": true,
      "index": true
    },
    {
      "label": "联系电话",
      "name": "contact_mobile",
      "type": "string",
      "comment": "联系电话",
      "nullable": true,
      "index": true
    },
    {
      "label": "详情",
      "name": "desc",
      "type": "text",
      "comment": "介绍",
      "nullable": true
    },
    {
      "label": "排序",
      "name": "rank",
      "type": "integer",
      "default": 9999999,
      "comment": "查询排序"
    },
    {
      "label": "状态",
      "name": "status",
      "type": "enum",
      "default": "enabled",
      "option": ["enabled", "disabled"],
      "comment": "状态 enabled 开启, disabled 关闭",
